def median(a,b,c):
    if a > b:
        if b > c:
            return b
        elif a > c:
            return c
        else :
            return a
    else:
        if b < c:
            return b
        elif a > c:
            return a
        else:
            return c
    

print(median(1,2,3))
#>>> 2

print(median(9,3,6))
#>>> 6

print(median(7,8,7))
#>>> 7
